{
  "version": "1.13.0",
  "configurations": [

    {
      "name": "(gdb) Lancer",
      "type": "cppdbg",
      "request": "launch",
      "program": "${workspaceFolder}/XMLParse/newVersion/parsing",
      "args": [
        "-m",
        "gold",
        "-o",
        "output55.json",
        "1G8H3Y9301N-Alignment.xml"
      ],
      "stopAtEntry": false,
      "cwd": "${fileDirname}",
      "environment": [],
      "externalConsole": false,
      "MIMode": "gdb",
      "miDebuggerPath": "/path/to/gdb",
      "setupCommands": [
        {
          "description": "Activer l'impression en mode Pretty pour gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        },
        {
          "description": "Définir la version désassemblage sur Intel",
          "text": "-gdb-set disassembly-flavor intel",
          "ignoreFailures": true
        }
      ]
    },
    {
      "name": "C/C++ Runner: Debug Session",
      "type": "cppdbg",
      "request": "launch",
      "args": [],
      "stopAtEntry": false,
      "externalConsole": true,
      "cwd": "c:/Users/ihsan/Desktop/ALTERNANTE/blast-project/XMLParse/newVersion",
      "program": "c:/Users/ihsan/Desktop/ALTERNANTE/blast-project/XMLParse/newVersion/build/Debug/outDebug",
      "MIMode": "gdb",
      "miDebuggerPath": "gdb",
      "setupCommands": [
        {
          "description": "Enable pretty-printing for gdb",
          "text": "-enable-pretty-printing",
          "ignoreFailures": true
        }
      ]
    }
  ]
}